//tipos de datos basicos en C++

#include <iostream>

using namespace std;
int main()
{
	int numeroEntero = 20; //variable es un espacio en memoria en donde voy a almacenar un valor determinado (en este caso nuermoEntero)//
	cout<<numeroEntero;
	
	float decimales = 23.56;
	cout<<decimales;
	
	double numerosExtensos = 45.6770;
	cout<<numerosExtensos;
	
	char tipoCaracter = 'a'; //puede ser numero o letra//
	cout<<tipoCaracter;
	
	return 0;
}
